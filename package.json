{
  "name": "usb",
  "description": "Library to access USB devices",
  "version": "1.2.2",
  "engines": {
    "node": ">=0.12.x"
  },
  "keywords": [
    "usb",
    "hardware"
  ],
  "contributors": [
    {
      "name" : "Stefan Berger",
      "email" : "stefan.berger@codefluegel.com"
    },
    {
      "name": "Kevin Mehall",
      "email": "km@kevinmehall.net",
