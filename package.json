    }
  ],
  "main": "./usb",
  "repository": {
    "type": "git",
    "url": "https://github.com/Timeular/nobe-usb.git"
  },
  "scripts": {
    "install": "prebuild-install --force || node-gyp rebuild",
    "test": "mocha --compilers coffee:coffee-script --grep Module",
    "full-test": "mocha --compilers coffee:coffee-script",
    "valgrind": "coffee -c test/usb.coffee; valgrind --leak-check=full --show-possibly-lost=no node --expose-gc --trace-gc node_modules/mocha/bin/_mocha -R spec",
    "ci": "node test/ci.js"
  },
  "dependencies": {
    "nan": "^2.4.0",
    "prebuild-install": "^5.0.0",
    "bindings": "~1.3.0"
  },
  "devDependencies": {
    "coffee-script": "~1.6.2",
    "mocha": "~1.8.2",
    "aws-sdk": "~2.0.0-rc.15"
  },
  "license": "MIT"
}
